# установка poetry
curl -sSL https://install.python-poetry.org | python -
# добавляем в PATH
PATH=$PATH:$HOME/.poetry/bin
poetry
# проверка работоспособности
brew install pyenv
# устанавливаю pyenv через brew
export PATH="$HOME/.pyenv/bin:$PATH"
# добавляю в PATH
pyenv
# проверка работоспособности

